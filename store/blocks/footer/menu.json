      "children": ["rich-text#address-brasil"]
  },
  "rich-text#address-brasil": {
      "props": {
          "text": "Avenida Andrômeda, 2000.<br />Bloco 6 e 8 Alphaville SP<br />R. Ifigenia Maria de Oliveira,<br />3793 - Jr. Piratininga Franca SP",
          "blockClass": "footer-address"
      }
  },

  "flex-layout.col#argentina": {
