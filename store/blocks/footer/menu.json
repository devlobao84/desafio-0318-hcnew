
{
  "footer": {
      "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": {
      "children": ["flex-layout.row#footer"]
  },
  "footer-layout.mobile": {
      "children": ["flex-layout.row#footer"]
  },
  "flex-layout.row#footer": {
      "props": {
          "blockClass": "footer",
          "horizontalAlign": "center",
          "fullWidth": true,
          "paddingTop": 6,
          "paddingBottom": 6
      },
      "children": [
          "flex-layout.col#countries",
          "flex-layout.col#footer-logo"
      ]
  },
  "flex-layout.col#countries": {
      "props": {
          "blockClass": "countries-container",
          "fullWidth": true,
          "width": "60%", 
          "paddingLeft": "2px"
      },
      "children": ["flex-layout.row#countries"]
  },
  "flex-layout.row#countries": {
      "props": {
          "blockClass": "countries-container"
      },
      "children": [
          "flex-layout.col#brasil",
          "flex-layout.col#argentina",
          "flex-layout.col#mexico",
          "flex-layout.col#chile"
      ]
  },

  "flex-layout.col#brasil": {
